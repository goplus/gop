func protect(g func()) {
	defer func() {
		println("done") // Println executes normally even if there is a panic
		if x := recover(); x != nil {
			println("run time panic:", x)
		}
	}()
	println("start")
	g()
}

defer func() {
	if v := recover(); v != nil {
		println("recover", v)
	}
	if v := recover(); v != nil {
		println("error", v)
	}
}()

protect(func() {
	i := 0
	println(1 / i)
})

panic("hello")
