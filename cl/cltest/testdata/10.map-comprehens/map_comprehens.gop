y := {x: i for i, x <- [1, 3, 5, 7, 11]}
println(y)

y = {x: i for i, x <- [1, 3, 5, 7, 11], i%2 == 1}
println(y)

z := {v: k for k, v <- {1: "Hello", 3: "Hi", 5: "xsw", 7: "Go+"}, k > 3}
